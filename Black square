def solve():
    a1, a2, a3, a4 = map(int, input().split())
    s = input()
    
    total_calories = 0
    for digit in s:
        if digit == '1':
            total_calories += a1
        elif digit == '2':
            total_calories += a2
        elif digit == '3':
            total_calories += a3
        elif digit == '4':
            total_calories += a4
            
    print(total_calories)

solve()
